style "scrollbar"
{
  engine "pixmap" 
  {
    image 
    {
      function			= BOX
      detail			= "trough"
      file			= "Scrollbars/trough-scrollbar-horiz.png"
      border			= { 19, 19, 4, 4 }
      stretch			= TRUE
      orientation		= HORIZONTAL
    }
    image 
    {
      function			= BOX
      detail			= "trough"
      file			= "Scrollbars/trough-scrollbar-vert.png"
      border			= { 4, 4, 19, 19 }
      stretch			= TRUE
      orientation		= VERTICAL
    }

# Sliders

    image 
    {
      function				= SLIDER
      state           			= NORMAL
      file				= "Scrollbars/slider-horiz.png" 
      border				= { 7, 7, 5, 5 }
      stretch				= TRUE
      orientation			= HORIZONTAL

    }
    image 
    {
      function				= SLIDER
